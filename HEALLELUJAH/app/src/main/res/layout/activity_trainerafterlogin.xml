<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".trainerafterlogin">

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="트레이너 전용"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="트레이너정보수정"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/onlyTrainer"
        app:layout_constraintStart_toStartOf="@+id/onlyTrainer"
        app:layout_constraintTop_toBottomOf="@+id/onlyTrainer" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="기본정보수정"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/infoTrainer"
        app:layout_constraintStart_toStartOf="@+id/infoTrainer"
        app:layout_constraintTop_toBottomOf="@+id/infoTrainer" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="쪽지"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/messageTrainer"
        app:layout_constraintStart_toStartOf="@+id/messageTrainer"
        app:layout_constraintTop_toBottomOf="@+id/messageTrainer" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="커리큘럼정보수정"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/curriculumTrainer"
        app:layout_constraintStart_toStartOf="@+id/curriculumTrainer"
        app:layout_constraintTop_toBottomOf="@+id/curriculumTrainer" />

    <ImageButton
        android:id="@+id/infoTrainer"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:backgroundTint="#7EC0EE"
        android:scaleType="fitCenter"
        android:tint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@drawable/ic_person_black_24dp" />

    <ImageButton
        android:id="@+id/messageTrainer"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="#7EC0EE"
        android:scaleType="fitCenter"
        android:tint="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/infoTrainer"
        app:layout_constraintTop_toTopOf="@+id/infoTrainer"
        app:srcCompat="@drawable/ic_mail_outline_black_24dp" />

    <ImageButton
        android:id="@+id/curriculumTrainer"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="100dp"
        android:backgroundTint="#7EC0EE"
        android:scaleType="fitCenter"
        android:tint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/infoTrainer"
        app:layout_constraintStart_toStartOf="@+id/infoTrainer"
        app:layout_constraintTop_toBottomOf="@+id/infoTrainer"
        app:srcCompat="@drawable/ic_fitness_center_black_24dp" />

    <ImageButton
        android:id="@+id/onlyTrainer"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:backgroundTint="#7EC0EE"
        android:scaleType="fitCenter"
        android:tint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/messageTrainer"
        app:layout_constraintStart_toStartOf="@+id/messageTrainer"
        app:layout_constraintTop_toTopOf="@+id/curriculumTrainer"
        app:srcCompat="@drawable/ic_directions_run_black_24dp" />

</androidx.constraintlayout.widget.ConstraintLayout>